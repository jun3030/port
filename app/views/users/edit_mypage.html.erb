

<div class="comment">
  <div class="comment-left">
    <div class="user-image"><%= image_tag @user.image, :size => '150x150' %></div>
  </div>
  <div class="comment-right">
    
    <h1 class="user-name-mypage"><%= @user.name %><span class="user-info">(<%= @user.prefecture %>,<%= @user.age %><%= @user.sex %>)</span></h1>
    <ul class="test">
      <li>担当パート:<%= @user.responsible_part %></li>
      <li>好きなジャンル:<%= @user.genre %></li>
      <li>好きなアーティスト:<%= @user.favorite_artist %></li>
      <li class="self-introduction"><%= @user.self_introduction %></li>
    </ul>
    
  </div>
</div>


<ul class="gallery" style="display:flex;flex-wrap:wrap;">
<% insta["media"]["data"].each do |test| %>
  <li style="list-style:none;margin:20px;padding:0;">
  <% if "mp4".in?(test["media_url"]) %>
    <%= video_tag test["media_url"], controls: true , :size => "281x281" %>
  <% else %>
    <%= image_tag test["media_url"], :size => "281x281" %>
  <% end %>
  </li>
<% end %>
</ul>


<div id="contentContainer" class="style-scope app-drawer" style="transition-duration: 200ms;" position="left" swipe-open="" opened="">
      
</div>